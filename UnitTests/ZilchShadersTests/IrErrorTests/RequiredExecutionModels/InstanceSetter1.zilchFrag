[UnitTest][Vertex]
struct InstanceSetter1
{
  function Main()
  {
    var helper = Helper();
    helper.Value = 1.0;
  }
}

struct Helper
{
  var Value : Real
  {
    get { return Helper.Kill(); }
    set { Helper.Kill(); }
  }
  
  [Static]
  function Kill() : Real
  {
    ShaderIntrinsics.Kill();
    return 1.0;
  }
}
